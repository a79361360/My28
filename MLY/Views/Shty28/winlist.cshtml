@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit">
    <link rel="Shortcut Icon" href="http://play28.mly.com/game/logo.ico">
    <link rel="Bookmark" href="http://play28.mly.com/game/logo.ico">
    <link href="/Content/css.css" rel="stylesheet" type="text/css">
    <script type="text/javascript" src="/js/jquery-1.js"></script>
    <script src="/js/PubPage.js" type="text/javascript"></script>

    <title>玩游戏赚金币，做任务换奖品，就在乐游网</title>
    <meta name="Keywords" content="游戏体验 免费 网赚 幸运28">
    <meta name="description" content="玩游戏赚金币，做任务换奖品，就在乐游网">

    <meta http-equiv="Content-Type" content="text/html; charset=gbk">
    <script>
        $(function(){
            $(".gamecontable tbody tr").each(function(i,o){
                var value = $(o).find("td").eq(0).text();
                value = "****"+value.substr(4,value.len);
                $(o).find("td").eq(0).text(value)
            });
        })
    </script>
</head>


<body>

    <div class="clear"></div>
    <div class="likebody mainbody">
        <div class="likebody">
           @Html.Action("RankRightList", "Shared", new { id = 1 })

            <div class="gamemain">

                <input value="50000000" id="maxzhi" type="hidden">
                @{
    if (ViewBag.Gameid == 1)
    {
        @Html.Action("top", "Shared", null)
    }
    else if (ViewBag.Gameid == 2)
    {
        @Html.Action("Top", "Bj28", null)
    }
}
                @Html.Raw(ViewBag.hidd)

                <div class="gamecontent">
                    <table class="gamecontable" align="center" cellpadding="0" cellspacing="1">
                        <thead>
                            <tr><th>用户ID</th><th width="230px">投注数量</th><th width="230px">获得数量</th></tr>
                        </thead>
                        <tbody>
                            @Html.Raw(ViewBag.html)
                        </tbody>
                    </table>
                    <div id="proPage">

                    </div>
                    <script type="text/javascript" language="javascript">
                        var pages = new createPage(@ViewData["pagesize"],"/Shty28/winlist?gameid=@Html.Raw(ViewBag.gameid)&id=@Html.Raw(ViewBag.id)&page=",@ViewData["total"], @ViewData["page"]);
                        pages.load();</script>
                </div>
            </div>



        </div>



        <div class="likefoot" style="text-align:center; margin-top:10px">
            <hr>
            <p style="text-align:center;">
                <span style="color:#454647;font-family:Verdana, Simsun, Helvetica, Arial, sans-serif;background-color:#F7F7F7;"></span>&nbsp;
            </p>
            <p style="text-align:center;">
                <span style="color:#454647;font-family:Verdana, Simsun, Helvetica, Arial, sans-serif;background-color:#F7F7F7;">健康游戏忠告：抵制不良游戏，拒绝盗版游戏；注意自我保护，谨防受骗上当；适度游戏益脑，沉迷游戏伤身；合理安排时间，享受健康生活。</span>
            </p>
    </div>
</body>
</html>