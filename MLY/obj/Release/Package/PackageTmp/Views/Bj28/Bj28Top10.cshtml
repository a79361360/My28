
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Bj28Top10</title>
</head>
<body>
    <input type="hidden" id="DqIssue" value="@Html.Raw(ViewBag.Iss)" />
    <div class="gamecontent">
        <table class="gamecontable" align="center" cellpadding="0" cellspacing="1">
            <thead>
                <tr>
                    <th width="">游戏期号</th>
                    <th width="">开奖时间</th>
                    <th width="">开奖号码</th>
                    <th width="">奖池总额</th>
                    <th width="">中奖人数</th>
                    <th width="99">盈亏</th>
                    <th>参与</th>
                </tr>
            </thead>
            <tbody id="Winning">
                @Html.Raw(ViewBag.IssTop3)
                @Html.Raw(ViewBag.IssTop7)
            </tbody>
        </table>
        <div id="proPage">

        </div>
        <script type="text/javascript" language="javascript">
            var pages = new createPage(@ViewData["pagesize"],"/Bj28/Index/",@ViewData["total"], @ViewData["page"]);
            pages.load();</script>
    </div>
</body>
</html>
